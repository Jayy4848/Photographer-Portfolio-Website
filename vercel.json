{
  "functions": {
    "photography_site/wsgi.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "photography_site/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "photography_site/wsgi.py"
    }
  ],
  "env": {
    "SECRET_KEY": "8blr7mqelv95-qy3@gu1t(!j26o*@gvwb1@=-ip_py^+a!7*6b",
    "DEBUG": "False",
    "ALLOWED_HOSTS": ".vercel.app,.vercel.com",
    "DJANGO_SETTINGS_MODULE": "photography_site.settings"
  }
} 